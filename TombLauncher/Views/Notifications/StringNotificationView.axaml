<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:TombLauncher.ViewModels"
             xmlns:notifications="clr-namespace:TombLauncher.ViewModels.Notifications"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="TombLauncher.Views.Notifications.StringNotificationView"
             x:DataType="notifications:StringNotificationViewModel"
             >
    <Design.DataContext>
        <notifications:StringNotificationViewModel/>
    </Design.DataContext>
    <TextBlock Text="{Binding Text}"/>
</UserControl>
